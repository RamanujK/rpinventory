Options -Indexes

# Show the viewInventory page by default
DirectoryIndex viewInventory.php index.php

# Block files in the directories
RewriteEngine On

RewriteRule \.git/ - [F] 
RewriteRule templates/ - [F]
RewriteRule cache/ - [F]
RewriteRule config/ - [F]
RewriteRule lib/ - [F]
RewriteRule modules/ - [F]
RewriteRule sql/ - [F]
RewriteRule templates_c/ [F]
